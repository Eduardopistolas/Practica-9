#include <stdio.h>

float suma(float a, float b);

float suma(float a, float b) {
    return a + b;
}

int main() {
    float a, b, resultado;

    printf("Ingrese el primer número: ");
    scanf("%f", &a);

    printf("Ingrese el segundo número: ");
    scanf("%f", &b);

    resultado = suma(a, b);

    printf("La suma es: %.2f\n", resultado);

    return 0;
}
